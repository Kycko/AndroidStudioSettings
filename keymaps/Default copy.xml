<keymap version="1" name="Default copy" parent="$default">
  <action id="ActivateLogcatToolWindow">
    <keyboard-shortcut first-keystroke="alt 6" />
  </action>
  <action id="ActivateTODOToolWindow" />
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="ctrl y" />
    <keyboard-shortcut first-keystroke="ctrl delete" />
  </action>
  <action id="EditorDeleteToWordEnd" />
  <action id="EditorLookupDown" />
  <action id="EditorLookupUp" />
  <action id="EditorMoveToPageBottom" />
  <action id="EditorMoveToPageTop" />
  <action id="EditorScrollDown" />
  <action id="EditorScrollUp" />
  <action id="EditorSelectWord" />
  <action id="EditorUnSelectWord" />
  <action id="MoveLineDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
    <keyboard-shortcut first-keystroke="ctrl down" />
  </action>
  <action id="MoveLineUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
    <keyboard-shortcut first-keystroke="ctrl up" />
  </action>
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="alt right" />
    <keyboard-shortcut first-keystroke="ctrl page_down" />
  </action>
  <action id="PreviousTab">
    <keyboard-shortcut first-keystroke="alt left" />
    <keyboard-shortcut first-keystroke="ctrl page_up" />
  </action>
  <action id="QuickJavaDoc">
    <keyboard-shortcut first-keystroke="ctrl q" />
    <mouse-shortcut keystroke="alt button2" />
    <keyboard-shortcut first-keystroke="ctrl back_slash" />
  </action>
  <action id="ReopenClosedTab">
    <keyboard-shortcut first-keystroke="shift ctrl w" />
  </action>
</keymap>